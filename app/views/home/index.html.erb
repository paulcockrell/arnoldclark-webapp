<form data-bind="submit: loadImages">
  <div class="form-group">
    <label for="registration">Registration number</label>
    <input data-bind="value: registration" id="registration" class="form-control" />
  </div>
  <div class="form-group">
    <label for="stock_reference">Stock reference number</label>
    <input data-bind="value: stock_reference" id="stock_reference" class="form-control" />
  </div>
  <div class="form-group">
    <button type="submit" value="submit" class="btn btn-primary">Load</button>
  </div>
</form>
